
import java.awt.*;
import javax.swing.*;

public class Ballgame extends JFrame {

	//窗口加载
	void launchFrame() {
		setSize(300,300);
		setLocation(400,400);
		setVisible(true);
	}
	
	
	//main 方法是程序执行的入口
	public static void main(String[]args) {
		System.out.println("我是张权威，这个游戏体验一下");
		Ballgame game = new Ballgame();
		game.launchFrame();
	}
}
